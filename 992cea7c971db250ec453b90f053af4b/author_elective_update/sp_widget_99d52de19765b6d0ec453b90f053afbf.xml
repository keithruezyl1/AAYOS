        <script><![CDATA[data.typeahead = $sp.getWidgetFromInstance('typeahead_search_header');
var AAYOS_KB_ID = "2a9ad1659725b6d0ec453b90f053af0a";

var kbId = AAYOS_KB_ID;

var gr = new GlideRecord("kb_knowledge");
gr.addQuery("kb_knowledge_base", kbId);
gr.addQuery("workflow_state", "published");
gr.query();

data.articles = [];
while (gr.next()) {
    data.articles.push({
        sys_id: gr.getUniqueValue(),
        number: gr.number.toString(),
        short_desc: gr.short_description.toString()
    });
}
]]></script>
